{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "plugins": [
        "react",
        "prettier"
    ],
    "rules": {
        "semi": [2, "always"],
        "comma-dangle": [2, "only-multiline"],
        "object-curly-spacing": ["error", "never"],
        "multiline-ternary": ["error", "always-multiline"],
        "prettier/prettier": "error"
    }
}